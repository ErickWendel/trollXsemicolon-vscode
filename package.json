{
    "name": "troll-x-semicolon",
    "displayName": "trollXsemicolon",
    "description": "Substitui todos os ponto e vírgula (`;`) pelo ponto de interrogação grego, caractere similar ao nosso ponto e virgula que não é reconhecido pelos nossos editores :D",
    "version": "0.1.0",
    "publisher": "eriickwendel",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "resources/greek-mark-ico.jpg",
    "activationEvents": [
        "onLanguage:csharp",
        "onLanguage:javascript",
        "onLanguage:java",
        "onLanguage:c"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}